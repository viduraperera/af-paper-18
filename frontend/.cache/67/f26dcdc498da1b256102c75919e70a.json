urce":"src/component/subjects/Subjects.js","original":{"line":38,"column":75}},{"generated":{"line":95,"column":0},"source":"src/component/subjects/Subjects.js","original":{"line":38,"column":75}},{"generated":{"line":95,"column":7},"source":"src/component/subjects/Subjects.js","original":{"line":38,"column":51}},{"generated":{"line":95,"column":8},"source":"src/component/subjects/Subjects.js","original":{"line":35,"column":12}},{"generated":{"line":95,"column":9},"source":"src/component/subjects/Subjects.js","original":{"line":34,"column":8}},{"generated":{"line":96,"column":0},"source":"src/component/subjects/Subjects.js","original":{"line":49,"column":5}},{"name":"Component","generated":{"line":100,"column":2},"source":"src/component/subjects/Subjects.js","original":{"line":4,"column":23}},{"name":"Subjects","generated":{"line":102,"column":15},"source":"src/component/subjects/Subjects.js","original":{"line":52,"column":15}}],"sources":{"src/component/subjects/Subjects.js":"import React, {Component} from 'react';\nimport axios from \"axios\";\n\nclass Subjects extends Component{\n    constructor(props) {\n        super(props);\n        this.state = {\n            subjects: [],\n            totalAmount: ''\n        }\n    }\n\n    componentDidMount() {\n        axios.get(`http://localhost:5001/courses/${this.props.match.params.id}`)\n            .then(response =>{\n                this.setState({ subjects: response.data})\n                console.log (response.data)\n            })\n            .catch(error =>{\n                alert(error.message);\n            })\n\n        axios.get(`http://localhost:5001/courses/amount/${this.props.match.params.id}`)\n            .then(response => {\n                this.setState({ totalAmount: response.data.totalAmount })\n                console.log(response.data.totalAmount)\n            })\n            .catch(error => {\n                alert(error.message)\n            })\n    }\n\n    render() {\n        return(\n            <div className=\"container\">\n                <h1>Course Subjects</h1>\n                <h3>Total Amount: {this.state.totalAmount}</h3>\n                {this.state.subjects.length > 0 && this.state.subjects.map((item, index) => (\n                    <div key={index} className=\"card mb-3\">\n                        <div className=\"p-3\">\n                            <h4>Subject Name: {item.name}</h4>\n                            <h5>Description: {item.description}</h5>\n                            <h5>Amount: {item.amount}</h5>\n                        </div>\n                    </div>\n                ))}\n            </div>\n        )\n    }\n}\n\nexport default Subjects;"},"lineCount":null}},"error":null,"hash":"0737baf1651d37f916e0ee7dec51193d","cacheData":{"env":{}}}